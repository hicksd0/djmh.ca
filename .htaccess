<IfModule mod_rewrite.c>
    RewriteEngine On
	RewriteCond $1 !^(index\.php|assets|assets/css|assets/js|assets/etc|robots\.txt)
    RewriteRule ^(.*)$ public/$1 [L]
</IfModule>

<FilesMatch "^\.">
    Require all denied
    Satisfy All
</FilesMatch>
